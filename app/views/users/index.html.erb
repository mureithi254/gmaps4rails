<p id="notice"><%= notice %></p>
<div class="">
<h1>Listing Users</h1>
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th><strong><em>Latitude</em></strong></th>
      <th><strong><em>Longitude</em></strong></th>
      <th><strong><em>Address</em></strong></th>
      <th><strong><em>Description</em></strong></th>
      <th><strong><em>Title</em></strong></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.latitude %></td>
        <td><%= user.longitude %></td>
        <td><%= user.address %></td>
        <td><%= user.description %></td>
        <td><%= user.title %></td>
        <td><%= link_to 'Show', user ,:class => 'btn btn-info' %></td>
        <td><%= link_to 'Edit', edit_user_path(user),:class => 'btn btn-success' %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' },:class => 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>

